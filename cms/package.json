    "karma-jasmine-html-reporter": "^1.4.0",
    "ng-packagr": "^9.0.0",
    "protractor": "~5.4.0",
    "rxjs-tslint-rules": "^4.33.1",
    "ts-node": "^7.0.0",
    "tslint": "^6.1.0",
    "typescript": "^3.5.3",
    "vrsource-tslint-rules": "^5.1.1"
