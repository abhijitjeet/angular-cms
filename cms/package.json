{
  "name": "cms",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "dev": "ng serve",
    "dev:aot": "ng serve --aot",
    "tslint": "tslint -c tslint.json --project ./tsconfig.json",
    "tslint:fix": "tslint --fix -c tslint.json --project ./tsconfig.json",
    "build:libs": "ng build core && ng build modules && ng build portal",
    "build:ssr": "ng build && ng run cms-demo:server",
    "build:ssr:prod": "ng build --prod && ng run cms-demo:server",
    "start:ssr": "ts-node -P ./cms-demo/server.tsconfig.json ./cms-demo/server.ts",
    "prerender": "ts-node -P ./cms-demo/server.tsconfig.json ./cms-demo/prerender.ts",
    "prerender:debug": "ts-node -P ./cms-demo/server.tsconfig.json --inspect ./cms-demo/prerender.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.13",
    "@angular/common": "^9.1.13",
    "@angular/compiler": "^9.1.13",
    "@angular/core": "^9.1.13",
    "@angular/forms": "^9.1.13",
    "@angular/platform-browser": "^9.1.13",
    "@angular/platform-browser-dynamic": "^9.1.13",
    "@angular/platform-server": "^9.1.13",
    "@angular/router": "^9.1.13",
    "@fortawesome/angular-fontawesome": "^0.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@nguniversal/common": "^9.0.2",
    "@nguniversal/express-engine": "^9.0.2",
    "angular-split": "^3.0.3",
    "bootstrap": "^4.3.1",
    "ngx-bootstrap": "^5.2.0",
    "ngx-quill": "^8.1.3",
    "ngx-wig": "^8.0.0",
    "quill": "^1.3.7",
    "quill-image-resize-module": "^3.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.4.0",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular/cli": "^9.1.13",
    "@angular/compiler-cli": "^9.1.13",
    "@angular/language-service": "^9.1.13",
